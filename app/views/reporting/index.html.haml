- cache do
  %div.js-reporting
    = render "shared/body-panel/search-component"
    .clearfix
    .row
      .col-md-12.col-sm-12.col-xs-12
        .x_panel
          .x_title
            %h2 Reporting (Demo)
            %ul.nav.navbar-right.panel_toolbox
              %li
                %a.collapse-link
                  %i.fa.fa-chevron-up
              %li.dropdown
                %a.dropdown-toggle{"aria-expanded" => "false", "data-toggle" => "dropdown", :href => "#", :role => "button"}
                  %i.fa.fa-wrench
                %ul.dropdown-menu{:role => "menu"}
                  %li
                    %a{:href => "#"} Settings 1
                  %li
                    %a{:href => "#"} Settings 2
              %li
                %a.close-link
                  %i.fa.fa-close
            .clearfix
          .x_content
            -# = debug @timesheets.as_json
            - if @timesheets.length == 0
              %h4 You don't have any submitted timesheets this week to view any reports          
            - else  
              -# ChartJS implementation
              %canvas{:id => "popChart"}

  -# Passing ruby data to FE
  = content_tag(:div, "", id: "rb_reporting_data", data: @data_hash)